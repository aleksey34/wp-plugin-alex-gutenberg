{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "alex-gutenberg/recent-post-dynamic-widget",
	"version": "0.1.0",
	"title": "Recent post widget",
	"category": "alex-gutenberg-widget",
	"icon": "flag",
	"description": "Widget - recent post",
	"supports": {
	  "html": false
	},
	"attributes": {
        "quantity":{
            "type":"number",
            "default": 2
        },
	  	"title":{
		  "type": "string",
		  "default": "Recent Post"
		}
    },
	"textdomain": "alex-theme",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
  }
